      "id": 1
    },
    {
      "type": "graph",
      "title": "Errores por Servicio",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=~'5..'}[5m])) by (job)",
          "legendFormat": "{{job}}",
          "refId": "B"
        }
      ],
      "datasource": "Prometheus",
      "id": 2
    }
  ]
