{
  "name": "@altamedica/doctors",
  "version": "1.0.0",
  "description": "ALTAMEDICA Doctors Management - Medical professionals platform",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "pnpm --dir ../../packages/types run build && pnpm --dir ../../packages/config-next run build && pnpm --dir ../../packages/shared run build && pnpm --dir ../../packages/firebase run build && pnpm --dir ../../packages/auth run build",
    "dev": "next dev --port 3002",
    "prebuild": "pnpm --dir ../../packages/types run build && pnpm --dir ../../packages/config-next run build && pnpm --dir ../../packages/shared run build && pnpm --dir ../../packages/firebase run build && pnpm --dir ../../packages/auth run build",
    "build": "npx next build",
    "start": "next start --port 3002",
    "signaling": "node scripts/start-signaling.js",
    "dev:full": "concurrently \"pnpm signaling\" \"pnpm dev\"",
    "dev:e2e": "concurrently \"pnpm --filter api-server dev\" \"pnpm dev\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . s,.tsx,.js,.jsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint . s,.tsx,.js,.jsx --fix --no-error-on-unmatched-pattern",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist coverage"
  },
  "dependencies": {
    "@altamedica/api-client": "workspace:*",
    "@altamedica/auth": "workspace:*",
    "@altamedica/database": "workspace:*",
    "@altamedica/diagnostic-engine": "workspace:*",
    "@altamedica/hooks": "workspace:*",
    "@altamedica/maps": "workspace:*",
    "@altamedica/medical": "workspace:*",
    "@altamedica/patient-services": "workspace:*",
    "@altamedica/shared": "workspace:*",
    "@altamedica/telemedicine-core": "workspace:*",
    "@altamedica/ui": "workspace:*",
    "@altamedica/utils": "workspace:*",
    "@anthropic-ai/sdk": "^0.55.1",
    "@modelcontextprotocol/sdk": "^1.13.2",
    "@next/bundle-analyzer": "^15.4.1",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@types/offscreencanvas": "^2019.7.3",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.536.0",
    "next": "15.3.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@altamedica/eslint-config": "workspace:*",
    "@next/eslint-plugin-next": "^15.3.4",
    "@playwright/test": "^1.54.2",
    "@types/node": "^20.19.11",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "autoprefixer": "^10.4.16",
    "date-fns": "^4.1.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.3",
    "firebase-admin": "^13.4.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "5.5.4"
  }
}