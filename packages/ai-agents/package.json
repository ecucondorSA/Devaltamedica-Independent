{
  "name": "@altamedica/ai-agents",
  "version": "1.0.0",
  "description": "Servicio de agentes de IA para AltaMedica",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --format esm --dts --clean ",
    "dev": "tsup src/index.ts --format=esm --watch",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@altamedica/shared": "workspace:*",
    "@altamedica/types": "workspace:*",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@types/minimatch": "^6.0.0",
    "@types/node": "^24.3.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "5.8.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "sideEffects": false
}
