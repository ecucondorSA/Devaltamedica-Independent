{
  "name": "@altamedica/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./services/logger.service": {
      "types": "./dist/services/logger.service.d.ts",
      "import": "./dist/services/logger.service.js",
      "require": "./dist/services/logger.service.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/services/logger.service.ts --format cjs --format esm --dts --clean",
    "dev": "tsup src/index.ts --format=esm --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "license": "MIT",
  "description": "Servicios compartidos para AltaMedica",
  "keywords": [
    "shared",
    "services",
    "logger",
    "medical"
  ],
  "dependencies": {
    "@altamedica/types": "workspace:*",
    "jspdf": "^2.5.1",
    "mercadopago": "^2.0.0",
    "qrcode": "^1.5.0",
    "speakeasy": "^2.0.0",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "5.8.3"
  },
  "sideEffects": false
}
