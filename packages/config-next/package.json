{
  "name": "@altamedica/config-next",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format=esm --watch",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "license": "MIT",
  "description": "Presets reutilizables para Next.js (headers HIPAA, utilidades de seguridad)",
  "keywords": [
    "next",
    "config",
    "hipaa",
    "security",
    "headers"
  ],
  "devDependencies": {
    "@types/node": "^20.19.11",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "5.8.3"
  }
}
