{
  "name": "@altamedica/auth",
  "version": "1.0.0",
  "description": "Paquete centralizado de autenticaci√≥n para AltaMedica",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    }
  },
  "private": true,
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts --format=esm --watch"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "dependencies": {
    "@altamedica/firebase": "workspace:*",
    "@altamedica/shared": "workspace:*",
    "@altamedica/types": "workspace:*",
    "firebase": "^10.7.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "otplib": "^12.0.1"
  },
  "devDependencies": {
    "@types/cookie": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.0 || ^19.0.0",
    "@types/react-dom": "^18.2.0 || ^19.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "5.8.3"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}
