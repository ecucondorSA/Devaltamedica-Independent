{
  "name": "@altamedica/anamnesis",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs --format esm --dts --clean ",
    "dev": "tsup src/index.ts --format=esm --watch",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "canvas-confetti": ">=1.6.0",
    "firebase": ">=10.0.0",
    "framer-motion": ">=10.0.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0",
    "react-type-animation": ">=3.0.0"
  },
  "dependencies": {
    "@altamedica/auth": "workspace:*",
    "@altamedica/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
