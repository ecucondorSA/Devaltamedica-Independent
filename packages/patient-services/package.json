{
  "name": "@altamedica/patient-services",
  "version": "1.0.0",
  "description": "AltaMedica patient-services package",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --format esm --dts --clean --silent",
    "dev": "tsup src/index.ts --format=esm --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@altamedica/types": "workspace:*"
  },
  "devDependencies": {
    "@altamedica/eslint-config": "workspace:*",
    "@types/jest": "^29.5.0",
    "@types/node": "^24.3.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "5.8.3",
    "vitest": "^1.0.0"
  },
  "sideEffects": false
}
