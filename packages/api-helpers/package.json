{
  "name": "@altamedica/api-helpers",
  "version": "1.0.0",
  "description": "API response helpers and utilities for AltaMedica",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --format esm --dts --clean ",
    "dev": "tsup src/index.ts --format=esm --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@altamedica/auth": "workspace:*",
    "resolve-from": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.4",
    "@types/express-rate-limit": "^6.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^24.3.0",
    "tsup": "^8.0.0",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "next": "^15.3.4"
  },
  "files": [
    "dist*",
    "README.md"
  ],
  "module": "./dist/index.mjs",
  "sideEffects": false
}
