{
  "name": "@altamedica/typescript-config",
  "version": "1.0.0",
  "description": "AltaMedica typescript-config package",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./base.json": "./base.json",
    "./react.json": "./react.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "5.5.4"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts"
}
