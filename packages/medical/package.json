{
  "name": "@altamedica/medical",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --format esm --dts --clean ",
    "dev": "tsup src/index.ts --format=esm --watch",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "dependencies": {
    "@altamedica/shared": "workspace:*",
    "date-fns": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "5.8.3"
  },
  "sideEffects": false
}
